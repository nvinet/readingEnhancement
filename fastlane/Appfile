# App identifier
app_identifier("com.nvinet.readingEnhancement")

# Apple ID and Team ID loaded from environment variables
# Set these in your shell profile or use a .env file (NOT committed to git)
# FASTLANE_USER=your-email@example.com
# FASTLANE_TEAM_ID=XXXXXXXXXX
# FASTLANE_ITC_TEAM_ID=XXXXXXXXXX (optional, if you have multiple teams)

apple_id(ENV["FASTLANE_USER"])
team_id(ENV["FASTLANE_TEAM_ID"])
itc_team_id(ENV["FASTLANE_ITC_TEAM_ID"]) if ENV["FASTLANE_ITC_TEAM_ID"]

